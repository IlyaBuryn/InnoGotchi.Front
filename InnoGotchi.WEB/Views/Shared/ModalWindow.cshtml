@using InnoGotchi.WEB.Extensions
@using InnoGotchi.WEB.Utility
@model Dictionary<string, string>
@{
    @if (Model.Count != 0)
    {
        var error = Model[nameof(ModalMsgType.ErrorMessage)];
        if (error != null)
        {
            @await Html.PartialAsync("_ModalPartial", new ModalWindow(ModalMsgType.ErrorMessage, error))
        }
        var success = Model[nameof(ModalMsgType.SuccessMessage)];
        if (success != null)
        {
            @await Html.PartialAsync("_ModalPartial", new ModalWindow(ModalMsgType.SuccessMessage, success))
        }
        var just = Model[nameof(ModalMsgType.JustMessage)];
        if (just != null)
        {
            @await Html.PartialAsync("_ModalPartial", new ModalWindow(ModalMsgType.JustMessage, just))
        }
    }
}
